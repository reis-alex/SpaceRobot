<?xml version="1.0" ?>

<robot name="pulsar">

  <!-- Colors -->
  <material name="Grey">
      <color rgba="0.2 0.2 0.2 1.0"/>
  </material>
  <material name="Orange">
      <color rgba="1.0 0.4 0.3 1.0"/>
  </material>
  <material name="Blue">
  <color rgba="0.5 0.7 1.0 1.0"/>
  </material>
  <material name="Red">
      <color rgba="1 0 0 1.0"/>
  </material>

<!-- ********************************************************** -->


  <!--Spacecraft-->
  <link name="Spacecraft">
      <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="100"/>
      <inertia ixx="40" ixy="0" ixz="0" iyy="20" iyz="0" izz="40"/>
      </inertial>
      <visual>
      <origin xyz="0. 0 0" rpy="0 0 0"/>
      <geometry>
          <box size="0.6 0.6 0.6"/>
      </geometry>
      <material name="Orange"/>
    </visual>
    <stiffness name="rigid"/>
  </link>


  
  <!-- ********************************************************** -->
  <!--joint Spacecraft to Reaction Wheel 1 -->
    <joint name="Spacecraft_to_RW1" type="revolute">
        <parent link="Spacecraft" />
        <child link="RW1"/>
        <origin xyz="0 0  0" rpy="pi/2 0 0" />
        <axis xyz="0 0 1"/>
        <limit effort="1000.0" lower="0" upper="0" velocity="0"/>
    </joint>

    <!-- Reaction Wheel 1 -->
    <link name="RW1" >
      <inertial>
          <origin rpy ="0 0 0" xyz="0 0 0.0" />
          <mass value="4"/>
          <inertia ixx="0.065" ixy="0" ixz="0" iyy="0.065" iyz="0" izz="0.1322"/>
      </inertial>
      <visual>
          <origin rpy="0 0 0" xyz="0 0 0"/>
          <geometry>
            <cylinder length="0.16/2" radius="0.31/2" />
          </geometry>
          <material name="Grey"/>
      </visual>
    <stiffness name="rigid"/>
    </link>

    <!--joint Spacecraft to Reaction Wheel 2 -->
    <joint name="Spacecraft_to_RW2" type="revolute">
        <parent link="Spacecraft" />
        <child link="RW2"/>
        <origin xyz="0 0 0" rpy="0 pi/2 0" />
        <axis xyz="0 0 1"/>
        <limit effort="1000.0" lower="0" upper="0" velocity="0"/>
    </joint>

    <!-- Reaction Wheel 2 -->
    <link name="RW2" >
      <inertial>
          <origin rpy ="0 0 0" xyz="0 0 0.0" />
          <mass value="4"/>
          <inertia ixx="0.065" ixy="0" ixz="0" iyy="0.065" iyz="0" izz="0.1322"/>
      </inertial>
      <visual>
          <origin rpy="0 0 0" xyz="0 0 0"/>
          <geometry>
            <cylinder length="0.16/2" radius="0.31/2" />
          </geometry>
          <material name="Grey"/>
      </visual>
    <stiffness name="rigid"/>
    </link>

    <!--joint Spacecraft to Reaction Wheel 3-->
    <joint name="Spacecraft_to_RW3" type="revolute">
        <parent link="Spacecraft" />
        <child link="RW3"/>
        <origin xyz="0 0 0" rpy="0 0 pi/2" />
        <axis xyz="0 0 1"/>
        <limit effort="1000.0" lower="0" upper="0" velocity="0"/>
    </joint>

    <!-- Reaction Wheel 3 -->
    <link name="RW3" >
    <inertial>
        <origin rpy ="0 0 0" xyz="0 0 0.0" />
        <mass value="4"/>
        <inertia ixx="0.065" ixy="0" ixz="0" iyy="0.065" iyz="0" izz="0.1322"/>
    </inertial>
    <visual>
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <geometry>
          <cylinder length="0.16/2" radius="0.31/2" />
        </geometry>
        <material name="Grey"/>
    </visual>
    <stiffness name="rigid"/>
    </link>

    

 <!-- ********************************************************** -->

  <!-- Joint to Manipulator -->
  <joint name="Spacecraft_Manipulator_Joint" type="fixed">
    <parent link="Spacecraft"/>
    <child link="Link_0"/>
    <origin rpy="0 0 0" xyz="0.25 0 0.375"/>
    <axis xyz="0 0 1"/>
  </joint>


 <!-- Link 0 -->
  <link name="Link_0">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.15/2"/>
      <mass value="1.97"/>
      <inertia ixx="35e-4" ixy="0" ixz="0" iyy="7e-4" iyz="0" izz="4e-4"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0*0.125"/>
      <geometry>
        <cylinder radius="0.05" length="0.15"/>
      </geometry>
      <material name="Grey"/>
    </visual>
  </link>

  <!-- Joint 1 -->
  <joint name="Joint_1" type="revolute">
    <parent link="Link_0"/>
    <child link="Link_1"/>
    <origin rpy="0 0 0" xyz="0 0 0.15/2"/>
    <axis xyz="0 0 1"/>
  </joint>
    <!-- Link 1 -->
  <link name="Link_1">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.1"/>
      <mass value="1.97"/>
      <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.05" iyz="0" izz="0.05"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.1"/>
      <geometry>
        <cylinder radius="0.05" length="0.2"/>
      </geometry>
      <material name="Blue"/>
    </visual>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder radius="0.1" length="0.1"/>
      </geometry>
      <material name="Orange"/>
    </visual>
  </link>



  <!-- Joint 2 -->
  <joint name="Joint_2" type="revolute">
    <parent link="Link_1"/>
    <child link="Link_2"/>
    <origin rpy="0 0 0" xyz="0 0 0.25"/>
    <axis xyz="1 0 0"/>
  </joint>
    <!-- Link 2 -->
  <link name="Link_2">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.375"/>
      <mass value="1.58"/>
      <inertia ixx="28e-4" ixy="0" ixz="0" iyy="7e-4" iyz="0" izz="4e-4"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.45"/>
      <geometry>
        <cylinder radius="0.05" length="1"/>
      </geometry>
      <material name="Blue"/>
    </visual>
    <!--<visual>
      <origin rpy="0 1.5708 0" xyz="0 0 0"/>
      <geometry>
        <cylinder radius="0.075" length="10.15"/>
      </geometry>
      <material name="Orange"/> 
    </visual>  -->
  </link>



  <!-- Joint 3 -->
  <joint name="Joint_3" type="revolute">
    <parent link="Link_2"/>
    <child link="Link_3"/>
    <origin rpy="0 0 0" xyz="0 0 1"/>
    <axis xyz="1 0 0"/>
  </joint>
    <!-- Link 3 -->
  <link name="Link_3">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.3"/>
      <mass value="1.17"/>
      <inertia ixx="7e-4" ixy="0" ixz="0" iyy="924e-4" iyz="0" izz="273e-4"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.25"/>
      <geometry>
        <cylinder radius="0.05" length="0.6"/>
      </geometry>
      <material name="Blue"/>
    </visual>
  </link>



  <!-- Joint End-Effector -->
  <joint name="Joint_EE" type="fixed">
    <parent link="Link_3"/>
    <child link="Link_EE"/>
    <origin rpy="0 0 0" xyz="0 0 0.6"/>
    <axis xyz="1 0 0"/>
  </joint>

    <!-- End-Effector -->
  <link name="Link_EE">
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value="2"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <!-- <sphere radius="0.4"/>  -->
        <box size="0.1 0.1 0.1"/>
      </geometry>
      <material name="Red"/>
    </visual>
  </link>
  



</robot>
