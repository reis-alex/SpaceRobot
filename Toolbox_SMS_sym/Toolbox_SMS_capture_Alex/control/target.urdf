<?xml version="1.0" ?>

<robot name="pulsar">

  <!-- Colors -->
  <material name="Grey">
      <color rgba="0.2 0.2 0.2 1.0"/>
  </material>
  <material name="Orange">
      <color rgba="1.0 0.4 0.3 1.0"/>
